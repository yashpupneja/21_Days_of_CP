#include<bits/stdc++.h>
using namespace std;

int main()
{
    int x=4;
    double d=4.0;
    string e="Hacktoberfest";
    int a;
    double b;
    string c;
    cin>>a;
    cin>>b;
    cin.ignore();
    getline(cin,c);
    cout<<a+x<<endl;
    cout<<fixed;
    cout<<setprecision(1)<<b+d<<endl;
    e=e+c;
    cout<<e<<endl;
    return 0;
}
